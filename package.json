{
  "name": "mongo-express-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prune": "rm -rf node_modules/",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --maxWorkers=50%"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ],
    "**/*.ts?(x)": "tsc-files --noEmit --pretty src/@types/global.d.ts src/@types/reset.d.ts src/@types/express/index.d.ts",
    "*.{json,yaml,md}": "prettier --write --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm test && pnpm build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "volta": {
    "node": "18.20.3",
    "yarn": "1.22.22"
  }
}